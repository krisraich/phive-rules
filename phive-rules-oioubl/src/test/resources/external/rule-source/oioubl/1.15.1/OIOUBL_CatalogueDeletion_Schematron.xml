			<sch:assert test="cbc:ID != ''">[F-CDE020] Invalid ID. Must contain a value</sch:assert>
			<!-- Validate -->
			<sch:report test="cbc:ContractType and cbc:ContractTypeCode">[F-CDE021] Use either ContractType or ContractTypeCode</sch:report>
