			<sch:assert test="count(cbc:LineCountNumeric) = 0">[F-CPU004] LineCountNumeric element must be excluded</sch:assert>
			<!-- Check for mandatory elements -->
			<sch:assert test="cbc:VersionID != ''">[F-CPU003] Invalid VersionID. Must contain a value</sch:assert>
			<sch:assert test="count(cac:SellerSupplierParty) = 1">[F-CPU005] One SellerSupplierParty class must be present</sch:assert>
			<!-- Validate -->
			<sch:assert test="cbc:ID != ''">[F-CPU002] Invalid ID. Must contain a value</sch:assert>
			<sch:report test="cac:ContractorCustomerParty and cac:ReceiverParty/cac:PartyIdentification/cbc:ID = cac:ContractorCustomerParty/cac:Party/cac:PartyIdentification/cbc:ID">[F-CPU006] ContractorCustomerParty must be different from ReceiverParty</sch:report>
			<sch:report test="count(cac:ValidityPeriod) &gt; 1">[F-CPU007] No more than one ValidityPeriod class must be present</sch:report>
			<sch:extends rule="DescriptionLanguageCheckNew"/>
		</sch:rule>

